Package: gfda
Depends:
    R (>= 3.1.0)
Imports:
    fda,
    splines,
    Rcpp (>= 0.11.3),
    RcppEigen (>= 0.3.2.2.0)
LinkingTo: Rcpp, RcppEigen
Type: Package
Title: Geostatistical Functional Data Analysis model
Version: 0.2.1
License: GPL (>= 2)
Authors@R: as.person(c(
    "Guilherme Ludwig <gvludwig@stat.wisc.edu> [aut, cre]",
